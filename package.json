{
  "name": "multiplayer-word-game",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:qa": "ng serve --configuration staging",
    "start:prod": "ng serve --configuration production",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "deploy:prod": "git push heroku-frontend-prod HEAD:master && git push heroku-backend-prod HEAD:master",
    "deploy:staging": "git push heroku-frontend-staging HEAD:master && git push heroku-backend-staging HEAD:master",
    "dev:ssr": "ng run MultiplayerWordGame:serve-ssr",
    "serve:ssr": "node dist-server/main.js",
    "build:ssr": "ng build && ng run MultiplayerWordGame:server",
    "prerender:production": "ng run MultiplayerWordGame:prerender --routes-file routes.txt",
    "prerender:staging": "ng run MultiplayerWordGame:prerender:staging --routes-file routes.txt",
    "start:heroku": "node dist-server/main.js",
    "heroku-postbuild": "ng build --configuration=$ENV && npm run prerender:$ENV"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.16",
    "@angular/cdk": "^12.2.13",
    "@angular/cli": "^12.2.16",
    "@angular/common": "^12.2.16",
    "@angular/compiler": "^12.2.16",
    "@angular/compiler-cli": "^12.2.16",
    "@angular/core": "^12.2.16",
    "@angular/forms": "^12.2.16",
    "@angular/platform-browser": "^12.2.16",
    "@angular/platform-browser-dynamic": "^12.2.16",
    "@angular/platform-server": "^12.2.16",
    "@angular/router": "^12.2.16",
    "@fortawesome/angular-fontawesome": "^0.10.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@nguniversal/express-engine": "^12.1.3",
    "@types/canvas-confetti": "^1.4.2",
    "@types/uuid": "^8.3.4",
    "angular-google-tag-manager": "1.4.2",
    "canvas-confetti": "^1.5.1",
    "express": "^4.15.2",
    "ngx-cookie-service": "^12.0.3",
    "ngx-socket-io": "4.1.0",
    "npm": "^8.5.4",
    "primeflex": "^3.1.2",
    "primeicons": "^5.0.0",
    "primeng": "^12.2.3",
    "rxjs": "^6.5.3",
    "socket.io-client": "^4.4.1",
    "staging": "^0.1.3",
    "tsconfig-paths": "^3.13.0",
    "tslib": "^2.3.0",
    "uuid": "^8.3.2",
    "webpack": "^5.70.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.16",
    "@angular/compiler-cli": "^12.2.16",
    "@fortawesome/fontawesome-free": "^6.0.0",
    "@nguniversal/builders": "^12.1.3",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.10.0",
    "@types/jquery": "^3.5.14",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "^1.0.0",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "schematics-scss-migrate": "^1.3.14",
    "typescript": "~4.3.5"
  }
}
